4 def test_deal_hand(): 
5     assert(len(deal_hand())) == 5 
6      
7 def test_pairs(): 
8     pair_hand = [Card(rank='9', suit='♥'), Card(rank='9', suit='♦'), Card(rank='10', suit='♦'), Card(rank='K', suit='♣'), Card(rank='J', suit='♦')] 
9     two_pair_hands = [Card(rank='9', suit='♥'), Card(rank='9', suit='♦'), Card(rank='10', suit='♦'), Card(rank='10', suit='♣'), Card(rank='J', suit='♦')] 
10     ranks = ['2', '3', '4', '5', '6'] 
11     assert find_pairs(ranks) == 0 
12     ranks = ['2', '2', '4', '5', '6'] 
13     assert find_pairs(ranks) == 1 
14     ranks = ['4', '5', '4', '5', '6'] 
15     assert find_pairs(ranks) == 2 
16      
17 def test_three_of_a_kind(): 
18     hand = [Card(rank='10', suit='♦'), Card(rank='K', suit='♦'), Card(rank='K', suit='♥'), Card(rank='K', suit='♠'), 
19              Card(rank='4', suit='♠')]     
20     assert three_of_a_kind(hand) == 3 
21      
22 def test_four_of_a_kind(): 
23     hand = [Card(rank='5', suit='♦'), Card(rank='5', suit='♦'), Card(rank='5', suit='♥'), Card(rank='5', suit='♠'), 
24              Card(rank='5', suit='2')] 
25     assert four_of_a_kind(hand) == 4 
26      
27 def test_five_of_a_kind(): 
28     hand = [Card(rank='3', suit='♦'), Card(rank='3', suit='♦'), Card(rank='3', suit='♥'), Card(rank='3', suit='♠'), 
29              Card(rank='3', suit='♠')] 
30     assert five_of_a_kind(hand) == 5 
31      
32 def test_straight(): 
33     hand = [Card(rank='A', suit='♦'), Card(rank='K', suit='♦'), Card(rank='Q', suit='♦'), Card(rank='J', suit='♦'), Card(rank='10', suit='♦')] 
34     assert straight(hand) == 1 
